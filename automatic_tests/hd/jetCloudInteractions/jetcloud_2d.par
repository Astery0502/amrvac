!=====================================================
! Project : 2D jet-cloud
! Aim     : de Gouveia dal pino, ApJ 526, 862, 1999
! Results :
! Update  : 31/08/2015
! configuration :
! $AMRVAC_DIR/setup.pl -d=22 -phi=0 -z=0 -g=16,16 -p=hd -eos=default -nf=1 -ndust=0 -u=nul -arch=default
!=====================================================

 &testlist
        teststr=''
 /

 &filelist
        filenamelog='jetCloud'
        filenameout='jetCloud'
        typefilelog='regression_test'
        primnames='rho v1 v2 p tr'
        saveprim=.true.
        autoconvert=.true.
        convert_type='vtuBCCmpi'
        typeparIO=-1
 /

 &savelist
        dtsave(1)=0.1
 /

 &stoplist
        dtmin=1.D-9
        tmax=3.0d0
 /

 &methodlist
        wnames= 'rho m1 m2 e tr'
        fileheadout=  'jetCloud_hd22'
        typeadvance=  'ssprk54'
        typefull1=    13*'hllc'
        typepred1=    13*'hllc'
        typelimiter1= 13*'mp5'
        typelimited='predictor'
        useprimitive=.true.
        dimsplit=.false.
        fixsmall=.true.
        strictsmall=.false.
        strictgetaux=.false.
        nflatgetaux=2
        smallp=1.0d-14
        smallrho=1.0d-14
 /

 &boundlist
        dixB=    3
        typeB=   5*'special',5*'cont',
                 5*'noinflow',5*'noinflow'
 /

 &amrlist
        errorestimate=3
        mxnest=2
	flags(6)=2
        flags(1)=1
	flags(2)=2
	wflags(1)=0.8
	wflags(2)=0.2
        nbufferx1=0
        nbufferx2=0
        tol=13*0.3d0
        tolratio=13*0.3d0
        nxlone1=80
        nxlone2=30
        xprobmin1=-16.0d0
        xprobmax1=16.0d0
        xprobmin2=-6.0d0
        xprobmax2=6.0d0
        iprob=1
 /

 &paramlist
        slowsteps=100
        typecourant='maxsum'
        courantpar=0.8d0
 /
