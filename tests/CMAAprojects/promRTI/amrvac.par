!setup.pl -d=23 -phi=0 -z=0 -g=16,16 -p=mhd

 &filelist
        filenamelog='datamr/promRTa'
        filenameout='datamr/promRTa'
        primnames= 'rho v1 v2 v3 p b1 b2 b3 psi'
        saveprim=.true.
        autoconvert=.true.
        convert_type='vtuBCCmpi'
        nwauxio=3
 /
        filenameini='datamr/promRTa'
        snapshotini=0
        snapshotnext=1
        firstprocess=.true.

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        dtsave=0.01d0,0.1d0
 /
        ditsave=1,10

 &stoplist
        dtmin=1.D-6
        tmax=7.5d0
 /
        itmax=20

 &methodlist
        wnames=       'rho m1 m2 m3 e b1 b2 b3 psi'
        fileheadout=  'promRT_mhd23'
        typeadvance=  'threestep'
        typefull1=    13*'hll'
        typepred1=    13*'hll'
        typelimiter1= 13*'cada3'
        typelimited='predictor'
        dimsplit=.false.
        ssplitdivb=.true.
        typedivbfix='glm2'
        fixsmall=.true.
        strictsmall=.true.
        strictgetaux=.false.
        nflatgetaux=4
        smallp=1.0d-14
        smallrho=1.0d-14
 /

 &boundlist
        dixB=    2
        typeB=   9*'periodic',9*'periodic',
                 8*'special','cont',8*'special','cont'
 /
'symm','asymm','symm','asymm','symm','symm','asymm','symm', 
                 'symm','asymm','symm','asymm','symm','symm','asymm','symm', 

 &amrlist
        errorestimate=3
        mxnest=4
        nbufferx1=0
        nbufferx2=0
        tol=13*0.2d0
        tolratio=13*0.1d0
        nxlone1=96
        nxlone2=96
        xprobmin1=0.0d0
        xprobmax1=1.0d0
        xprobmin2=0.0d0
        xprobmax2=1.0d0
 /

 &paramlist
        typecourant='maxsum'
        courantpar=0.8d0
        slowsteps=10
 /
