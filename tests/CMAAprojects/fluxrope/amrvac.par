!setup.pl -d=23 -phi=0 -z=0 -g=14,14 -p=mhd

 &filelist
        filenamelog='datamr/fluxrope23glma'
        filenameout='datamr/fluxrope23glma'
        primnames='rho v1 v2 v3 p b1 b2 b3 psi'
        saveprim=.true.
        autoconvert=.true.
        convert_type='vtuBCCmpi'
        nwauxio=9
 /
        filenameini='datamr/fluxrope23glma'
        snapshotini=48
        snapshotnext=49

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        dtsave=0.01d0,0.2d0
 /
        ditsave=1,100

 &stoplist
        dtmin=1.D-7
        tmax=26.d0
 /
        tmax=2.d0

 &methodlist
        wnames=       'rho m1 m2 m3 e b1 b2 b3 psi'
        fileheadout=  'fr_mhd23'
        typeadvance=  'threestep'
        typefull1=    13*'hll'
        typepred1=    13*'hll'
        typelimiter1= 13*'cada3'
        typelimited='predictor'
        dimsplit=.false.
        ssplitdivb=.true.
        typedivbfix='glm2'
        fixsmall=.true.
        strictsmall=.false.
        strictgetaux=.true.
        nflatgetaux=4
        smallp=1.0d-4
        smallrho=1.0d-14
 /

 &boundlist
        dixB=    2
        typeB=	 'symm','asymm','symm','asymm','symm','asymm','symm','asymm','symm',
                 'symm','asymm','symm','asymm','symm','asymm','symm','asymm','symm',
                 8*'special','cont',8*'special','cont'
 /

 &amrlist
        errorestimate=3
        mxnest=4
        nbufferx1=0
        nbufferx2=0
        flags(10)=4
        flags(1)=1
        flags(2)=6
        flags(3)=7
        flags(4)=8
        wflags(1)=0.4d0
        wflags(2)=0.2d0
        wflags(3)=0.2d0
        wflags(4)=0.2d0
        tol=13*0.12d0
        tolratio=13*0.1d0
        nxlone1=100
        nxlone2=300
        iprob=2
        xprobmin1=-1.d0
        xprobmax1=1.d0
        xprobmin2=0.d0
        xprobmax2=6.d0
        ditregrid=2
 /
        typeprolonglimit='koren'
        amrentropy= T

 &paramlist
        typecourant='maxsum'
        courantpar=0.8d0
 /
