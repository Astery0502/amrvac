!setup.pl -d=22 -g=16,16 -p=hd

 &filelist
        primnames= 'rho v1 v2 p'
        saveprim=.true.
        autoconvert=.true.
	convert_type='vtuBCCmpi'
/

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        dtsave=0.01,0.05
 /
        ditsave(1)=1

 &stoplist
        dtmin=1.0d-6
        tmax=5.0d0
 /

 &methodlist
        wnames=       'rho m1 m2 e'
        fileheadout=  'hd22'
        typeadvance=  'threestep'
        typefull1=13*'hllc'
        typelimiter1='cada3'

 /

 &boundlist
        dixB=2
        typeB=4*'cont',4*'cont',4*'cont',4*'cont'
 /

 &amrlist
        errorestimate=3
        mxnest=3
        nxlone1=120
        nxlone2=120
        xprobmin1=0.0d0
        xprobmax1=1.0d0
        xprobmin2=0.0d0
        xprobmax2=1.0d0
 /

 &paramlist
        courantpar=0.6d0
 /
        slowsteps=10
