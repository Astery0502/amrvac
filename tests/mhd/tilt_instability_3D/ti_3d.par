 &filelist
        base_filename='ti_3d'
        typefilelog='regression_test'
        autoconvert   = T
        saveprim      = T
        nwauxio       = 1
/

 &savelist
        dtsave_log    = 1.d-3
 /

 &stoplist
        time_max     = 5.d-2
 /

 &methodlist
        time_integrator   = 'threestep'
        flux_scheme     = 20*'hll'
        limiter  = 20*'cada3'
 /

 &boundlist
        nghostcells          = 2
        typeboundary_min1= 8*'special'
        typeboundary_max1= 8*'special'
        typeboundary_min2= 8*'special'
        typeboundary_max2= 8*'special'
        typeboundary_min3= 8*'periodic'
        typeboundary_max3= 8*'periodic'
 /

 &meshlist
        refine_criterion = 3
        refine_max_level=2
        w_for_refine(9)=3
        w_for_refine(1)=1
        w_for_refine(2)=6
        w_for_refine(3)=7
        w_refine_weight(1)=0.4d0
        w_refine_weight(2)=0.3d0
        w_refine_weight(3)=0.3d0
        refine_threshold=1*0.2d0
        derefine_ratio=1*0.1d0
        domain_nx1       = 64
        domain_nx2       = 64
        domain_nx3       = 48
        xprobmin1     = -3.0d0
        xprobmax1     = 3.0d0
        xprobmin2     = -3.0d0
        xprobmax2     = 3.0d0
        xprobmin3     = -1.0d0
        xprobmax3     = 1.0d0
 /

 &paramlist
        typecourant='maxsum'
        courantpar=0.9d0
        slowsteps=10
 /
