!setup.pl -d=2
 &filelist
        base_filename='bw_cartesian_2D'
        saveprim=.true.
        convert_type='vtuBCCmpi'
        autoconvert=.true.
        nwauxio=3
        typeparIO=-1
 /

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        ditsave_log=1
        dtsave_dat=0.05d0
 /

 &stoplist
        time_max=0.15d0
/

 &methodlist
        time_integrator=  'threestep'
        flux_scheme=    20*'hll'
        limiter= 20*'koren'
        small_pressure=1.0d-14
        small_density=1.0d-14
 /

 &boundlist
        nghostcells=    2
        typeboundary_min1=6*'cont'
        typeboundary_max1=6*'cont'
        typeboundary_min2=6*'cont'
        typeboundary_max2=6*'cont'
 /

 &meshlist
        refine_criterion=3
        refine_max_level=3
        w_for_refine(7)=2
        w_for_refine(1)=1
        w_for_refine(2)=4
        w_refine_weight(1)=0.5d0
        w_refine_weight(2)=0.5d0
        refine_threshold=20*0.2d0
        block_nx1=16
        block_nx2=16
        domain_nx1=32
        domain_nx2=32
        iprob=1 
        xprobmin1=0.d0
        xprobmax1=2.d0
        xprobmin2=0.d0
        xprobmax2=2.d0
 /
        ditregrid=2
        typeprolonglimit='koren'
        amrentropy= T

 &paramlist
        typecourant='maxsum'
        courantpar=0.5d0
 /

 &mhd_list
        Busr=1.d0
 /
