!setup.pl -d=2
 &filelist
        typefilelog='regression_test'
        base_filename='bw_2d'
        primnames='rho v1 v2 p b1 b2'
        saveprim=.true.
        convert_type='vtuBCCmpi'
        autoconvert=.true.
        nwauxio=3
        typeparIO=-1
 /
        restart_from_file='blastwavemhda'
        snapshotini=1
        snapshotnext=2
        convert=.true.

 &savelist
        dtsave_log       = 1.d-3
 /

 &stoplist
        time_max            = 5.d-2
/

 &methodlist
        w_names=       'rho m1 m2 e b1 b2'
        time_integrator=  'threestep'
        flux_scheme=    20*'hll'
        limiter= 20*'koren'
        small_pressure=1.0d-14
        small_density=1.0d-14
 /

 &boundlist
        nghostcells=    2
        typeboundary_min1=6*'cont'
        typeboundary_max1=6*'cont'
        typeboundary_min2=6*'cont'
        typeboundary_max2=6*'cont'
 /

 &meshlist
        refine_criterion=3
        refine_max_level=3
        w_for_refine(7)=2
        w_for_refine(1)=1
        w_for_refine(2)=4
        w_refine_weight(1)=0.5d0
        w_refine_weight(2)=0.5d0
        refine_threshold=20*0.2d0
        block_nx1=16
        block_nx2=16
        domain_nx1=32
        domain_nx2=32
        iprob=1 
        xprobmin1=0.d0
        xprobmax1=2.d0
        xprobmin2=0.d0
        xprobmax2=2.d0
 /
        ditregrid=2
        typeprolonglimit='koren'
        amrentropy= T

 &paramlist
        typecourant='maxsum'
        courantpar=0.5d0
 /

 &mhd_list
        Busr=1.d0
 /
