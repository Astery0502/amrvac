!setup.pl -d=33

&filelist
  primnames        = 'rho v1 v2 v3 p b1 b2 b3'
  typefilelog      = 'regression_test'
  autoconvert      = .true.
  convert_type     = 'vtuBCCmpi'
  saveprim         = .true.
/

 &savelist
   dtsave_log       = 1.d-3
/

 &stoplist
   time_max            = 5.d-2
/

 &methodlist
   w_names          = 'rho m1 m2 m3 e b1 b2 b3'
   time_integrator     = 'threestep'
   flux_scheme       = 20*'hll'
   limiter    = 20*'cada3'
/
 &boundlist
  nghostcells  =   2
  typeboundary_min1 = 8*'cont'
  typeboundary_max1 = 8*'cont'
  typeboundary_min2 = 8*'cont'
  typeboundary_max2 = 8*'cont'
  typeboundary_min3 = 8*'cont'
  typeboundary_max3 = 8*'cont'
/
 &meshlist
   refine_criterion   = 3
   refine_max_level          = 3
   block_nx1        = 4
   block_nx2        = 4
   block_nx3        = 4
   domain_nx1         = 8
   domain_nx2         = 8
   domain_nx3         = 8
   xprobmin1       =-1.0d0
   xprobmax1       = 1.0d0
   xprobmin2       =-1.0d0
   xprobmax2       = 1.0d0
   xprobmin3       =-1.0d0
   xprobmax3       = 1.0d0
 /

 &paramlist
  slowsteps        = 10
 /
 &mhd_list
  mhd_thermal_conduction =T
 /
