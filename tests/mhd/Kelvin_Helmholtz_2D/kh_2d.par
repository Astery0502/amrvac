!=====================================================
! Project : Kelvin-Helmholtz problem
! Aim     : N.C.
! Results : 
! Update  : 
! configuration :
! -d=2
!=====================================================
 
 &filelist
        typefilelog='regression_test'
        filenamelog='kh_2d'
        filenameout='kh_2d'
        primnames='rho v1 v2 p b1 b2'
        convert_type='vtuBCCmpi'
        saveprim=.true.
        autoconvert=.true.
        nwauxio=1
 /
 
 &savelist
   dtsave(1)       = 1.d-3
/

 &stoplist
   tmax            = 5.d-2
/
 
 &methodlist
        wnames=       'rho m1 m2 e b1 b2'
        fileheadout=  'KH_mhd22'
        typeadvance=  'threestep'
        typefull1=    13*'hll'
        typelimiter1= 13*'cada3'
        typelimited ='predictor'
        useprimitive=.true.
 /
 &boundlist
        dixB  =   2
        typeB = 6*'periodic', 6*'periodic',6*'cont',6*'cont'
 /
 
 
 &amrlist
        errorestimate=3
        flags(7)=3
        flags(1)=1
        flags(2)=2
        flags(3)=3
        wflags(1)=0.4d0
        wflags(2)=0.3d0
        wflags(3)=0.3d0
        mxnest=3
        tol=13*0.2d0
        nxblock1=12
        nxblock2=12
        nxlone1=48
        nxlone2=48
        xprobmin1=0
        xprobmax1=4.0d0
        xprobmin2=0
        xprobmax2=4.0d0
        iprob=2 
 /
 
 &paramlist
        typecourant='maxsum'
        slowsteps=10
        courantpar=0.8d0
 /
