! setup.pl -d=3
 &filelist
        typefilelog='regression_test'
	base_filename='bw_3d'
        primnames='rho v1 v2 v3 p b1 b2 b3'
        autoconvert=.true.
        saveprim=.true.
        nwauxio=3
	convert_type='vtuBmpi'
 /

 &savelist
        dtsave_log       = 1.d-3
 /

 &stoplist
        time_max         = 5.d-2
 /

 &methodlist
        w_names='rho m1 m2 m3 e b1 b2 b3'
        time_integrator='threestep'
        flux_scheme=20*'hll'
        limiter=20*'koren'
 /

 &boundlist
        nghostcells=2
        typeboundary_min1=8*'cont'
        typeboundary_max1=8*'cont'
        typeboundary_min2=8*'cont'
        typeboundary_max2=8*'cont'
        typeboundary_min3=8*'periodic'
        typeboundary_max3=8*'periodic'
 / 

 &meshlist
        refine_criterion=3
        refine_max_level=3
        w_for_refine(9)=2
        w_for_refine(1)=1
        w_for_refine(2)=5
        w_refine_weight(1)=0.5
        w_refine_weight(2)=0.5
        block_nx1=8
        block_nx2=8
        block_nx3=8
        domain_nx1=16
        domain_nx2=8
        domain_nx3=16
        xprobmin1=1.d0
        xprobmax1=3.d0
        xprobmin2=0.02d0
        xprobmax2=0.48d0
        xprobmin3=0.d0
        xprobmax3=1.d0
 /

 &paramlist
        typecourant='maxsum'
        courantpar=0.5d0
 /

 &mhd_list
        Busr=1.d0
 /
