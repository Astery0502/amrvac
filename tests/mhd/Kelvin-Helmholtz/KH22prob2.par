!=====================================================
! Project : Kelvin-Helmholtz problem
! Aim     : N.C.
! Results : 
! Update  : 
! configuration :
! -d=22 -g=16,16 -p=mhd
!=====================================================
 
 &filelist
        filenamelog='datamr/KH2proba'
        filenameout='datamr/KH2proba'
        saveprim=.true.
        primnames='rho v1 v2 p b1 b2 tr'
        convert_type='vtuCCmpi'
        nwauxio=1
        autoconvert=.true.
 /
        filenameini='datamr/KH2proba'
        snapshotini=0
        snapshotnext=1
        convert=.true.
 
 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        dtsave=0.01d0,0.5d0
 /
        ditsave=1,10
 
 &stoplist
        tmax=8.d0
        dtmin=1.0D-9
 /
 
 &methodlist
        wnames=       'rho m1 m2 e b1 b2 tr'
        fileheadout=  'KH_mhd22'
        typeadvance=  'threestep'
        typefull1=    13*'hllc'
        typepred1=    13*'hllc'
        typelimiter1= 13*'cada3'
        typelimited ='predictor'
        useprimitive=.true.
        smallp=1.0d-14
        smallrho=1.0d-13
        strictsmall=.true.
        strictgetaux=.false.
        typedivbfix='linde'
 /
 &boundlist
        dixB  =   2
        typeB = 7*'periodic', 7*'periodic',7*'cont',7*'cont'
 /
 
 
 &amrlist
        errorestimate=3
        flags(8)=3
        flags(1)=1
        flags(2)=2
        flags(3)=3
        wflags(1)=0.4d0
        wflags(2)=0.3d0
        wflags(3)=0.3d0
        mxnest=4
        tol=13*0.2d0
        nxlone1=48
        nxlone2=48
        xprobmin1=0
        xprobmax1=4.0d0
        xprobmin2=0
        xprobmax2=4.0d0
        iprob=2 
 /
 
 &paramlist
        typecourant='maxsum'
        slowsteps=10
        courantpar=0.8d0
 /
