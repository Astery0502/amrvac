 &filelist
        autoconvert   = T
        saveprim      = T
        nwauxio       = 0
        base_filename = 'output/method_test'
        convert_type  = 'vtuBmpi'
 /

 &savelist
        dtsave_dat    = 1000.0d0
        time_between_print = 10.0
 /

 &stoplist
        time_max      = 10.5
 /

 &methodlist
        time_integrator = 'threestep'
        flux_scheme     = 20*'hll'
        limiter  = 20*'cada3'
 /

 &boundlist
        typeboundary_min1  = 8*'periodic'
        typeboundary_min2  = 8*'periodic'
        typeboundary_min3  = 8*'periodic'
        typeboundary_max1  = 8*'periodic'
        typeboundary_max2  = 8*'periodic'
        typeboundary_max3  = 8*'periodic'
 /

 &meshlist
        iprob = 5
        refine_criterion = 3
        refine_max_level = 1
        domain_nx1    = 16
        domain_nx2    = 16
        domain_nx3    = 16
        xprobmin1     = -5.0d8
        xprobmax1     = 5.0d8
        xprobmin2     = -5.0d8
        xprobmax2     = 5.0d8
        xprobmin3     = -5.0d8
        xprobmax3     = 5.0d8
 /

 &paramlist
        courantpar    = 0.8d0
 /

 &mhd_list
        typedivbfix   = 'linde'
        mhd_particles = .true.
 /

 &particles_list
        physics_type_particles='Lorentz'
        num_particles=1
        dtsave_particles = 1.0d-2
        write_ensemble = t
        write_individual = t
        npayload=1
        particles_eta=0.0d-4
        !const_dt_particles = 0.1d0
 /

 &my_list
        charge = 1.
 /