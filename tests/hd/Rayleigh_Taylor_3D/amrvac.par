!setup.pl -d=3

&filelist
  primnames        = 'rho v1 v2 v3 p'
  filenamelog      = 'rthd_3d'
  filenameout      = 'rthd_3d'
  autoconvert      = .true.
  convert_type     = 'vtuBCCmpi'
  saveprim         = .true.
/

 &savelist
   dtsave(1)       = 1.d-3
   dtsave(2)       = 1.d-1
/

 &stoplist
   tmax            = 5.d-1
/

 &methodlist
   wnames          = 'rho m1 m2 m3 e'
   fileheadout     = 'rt_3d'
   typeadvance     = 'threestep'
   typefull1       = 13*'hllc'
   typelimiter1    = 13*'cada3'
   useprimitive    = T
/
 &boundlist
  dixB  =   2
  typeB =         5*'periodic', 5*'periodic',
                 'symm','symm','asymm','symm','symm',
                 'symm','symm','asymm','symm','symm',
                  5*'periodic', 5*'periodic'
/
 &amrlist
   errorestimate   = 3
   mxnest          = 4
   nxblock1        = 4
   nxblock2        = 4
   nxblock3        = 4
   nxlone1         = 8
   nxlone2         = 8
   nxlone3         = 8
   xprobmin1       = 0.0d0
   xprobmax1       = 1.0d0
   xprobmin2       = 0.0d0
   xprobmax2       = 1.0d0
   xprobmin3       = 0.0d0
   xprobmax3       = 1.0d0
 /

 &paramlist
  slowsteps        = 10
 /

 &hd_list
  hd_gravity = .true.
 /
