
 &filelist
        filenameout='datamr/liskaisotr'
        filenamelog='datamr/liskaisotr'
        primnames= 'rho v1 v2 tr'
        saveprim=.true.
        autoconvert=.true.
/

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        ditsave(1)=1
        dtsave(2)=0.1
 /

 &stoplist
        tmax=2.5
        dtmin=1.0d-9
 /

 &methodlist
        wnames=       'rho m1 m2 Dtr'
        typeadvance=  'threestep'
        typefull1=13*'hll'
        typelimiter1=13*'cada3'
	dimsplit=F
 /

 &boundlist
        dixB=2
        typeB='symm' 'asymm' 'symm' 'symm'
        'symm' 'asymm' 'symm' 'symm'
        'symm' 'symm' 'asymm' 'symm'
        'symm' 'symm' 'asymm' 'symm'
 /

 &amrlist
        errorestimate=3
        mxnest = 4
        nxlone1=100
        nxlone2=100
        xprobmin1=0.
        xprobmax1=0.3
        xprobmin2=0.
        xprobmax2=0.3
 /

 &paramlist
        courantpar=0.6d0
        slowsteps=10
 /
