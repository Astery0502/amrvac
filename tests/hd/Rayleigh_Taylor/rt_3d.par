 &testlist
 /

 &filelist
	autoconvert    = .true.
        saveprim       = .true.
        primnames      = 'rho v1 v2 v3 p'
        convert_type   = 'vtuBCCmpi'
        filenameout    = 'rt_3d_' ! Base file name for dat/vtu output
/

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        itsave(1,5)=0
        dtsave(1)=0.1d0         ! For the log
        dtsave(2)=0.1d0         ! For dat/vtu output
        dtsave(5)=0.1d0         ! For analysis output
 /

 &stoplist
        tmax=2.0d0              ! Stop after this time
        dtmin=1.D-7             ! Stop when dt < dtmin
 /

 &methodlist
        wnames=       'rho m1 m2 m3 e'
        typeadvance=  'threestep'
        typefull1=    13*'hllc'
        typelimiter1= 13*'koren'
 /

 &boundlist
        dixB=    2
        typeB=    5*'periodic',
                  5*'periodic',
                 'symm','symm','asymm','symm','symm',
                 'symm','symm','asymm','symm','symm'
                  5*'periodic',
                  5*'periodic'
 /

 &amrlist
        errorestimate=3
        mxnest=2
        nbufferx1=0
        nbufferx2=0
        tol=13*0.1d0
        nxlone1=32
        nxlone2=32
        nxlone3=32
        xprobmin1=0.0d0
        xprobmax1=1.0d0
        xprobmin2=0.0d0
        xprobmax2=1.0d0
        xprobmin3=0.0d0
        xprobmax3=1.0d0
 /

 &paramlist
        typecourant='maxsum'
        courantpar= 0.9d0
        slowsteps=10
 /
