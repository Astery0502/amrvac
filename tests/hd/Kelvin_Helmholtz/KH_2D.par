! $AMRVAC_DIR/setup.pl -d=22 -g=16,16 -p=hd -u=nul .......

&testlist
        teststr=''
/

&filelist
     filenamelog='./data/khhd'
     base_filename='./data/khhd'
  primnames        = 'rho v1 v2 p'
  autoconvert      = T
  convert_type     = 'vtuBmpi'
  saveprim         = T
/

 &savelist
   itsave(1,1)     = 0
   itsave(1,2)     = 0
   itsave(1,5)     = 0
   dtsave(1)       = 1.d-1
   dtsave(2)       = 2.d-1
   dtsave(5)       = 1.d-1
/

 &stoplist
   time_max=3.0d0
/

 &methodlist
   w_names          = 'rho m1 m2 e'
   fileheadout     = '2Dhd'
   time_integrator     = 'threestep'
   flux_scheme       = 20*'hllc'
   limiter    = 20*'cada3'
        useprimitive= .true.
        dimsplit    = .false.
        fixsmall=.true.
        strictsmall=.true.
        strictgetaux=.true.
        nflatgetaux=2
        small_pressure=-1.0d0
        small_density=-1.0d0

/   
       loglimit(1) = .true.
        loglimit(4) = .true.
 &boundlist
  nghostcells  =   3
  typeB=
                  4*'periodic',
                  4*'periodic',
                  4*'periodic',
                  4*'periodic'
/
 &meshlist
   refine_max_level          = 1
   domain_nx1         = 50
   domain_nx2         = 50
   xprobmin1       = 0.0d0
   xprobmax1       = 1.0d0
   xprobmin2       = 0.0d0
   xprobmax2       = 1.0d0
   iprob=1
 /
  
 &paramlist
  slowsteps        = 10
  courantpar       = 1.2d0
  typecourant      = 'maxsum'
 /
