&filelist
  base_filename='kh_2d'
  typefilelog = 'regression_test'
  primnames        = 'rho v1 v2 p'
  autoconvert      = .false.
  convert_type     = 'vtuBCCmpi'
  saveprim         = .true.
/

 &savelist
   dtsave_log       = 1.d-1
/

 &stoplist
   time_max=2.0d0
/

 &methodlist
   w_names          = 'rho m1 m2 e'
   time_integrator     = 'threestep'
   flux_scheme       = 20*'hllc'
   limiter    = 20*'cada3'
        dimsplit    = .false.
        small_pressure=-1.0d0
        small_density=-1.0d0

/

 &boundlist
  nghostcells  =   3
  typeB=
                  4*'periodic',
                  4*'periodic',
                  4*'periodic',
                  4*'periodic'
/
 &meshlist
   refine_max_level          = 1
   domain_nx1         = 32
   domain_nx2         = 32
   block_nx1        = 16
   block_nx2        = 16
   xprobmin1       = 0.0d0
   xprobmax1       = 1.0d0
   xprobmin2       = 0.0d0
   xprobmax2       = 1.0d0
   iprob=1
 /

 &paramlist
  slowsteps        = 10
  courantpar       = 1.2d0
  typecourant      = 'maxsum'
 /
