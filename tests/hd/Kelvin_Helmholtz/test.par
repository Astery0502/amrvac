&filelist
  filenamelog = 'kh_2d'
  typefilelog = 'regression_test'
  primnames        = 'rho v1 v2 p'
  autoconvert      = .false.
  convert_type     = 'vtuBCCmpi'
  saveprim         = .true.
/

 &savelist
   dtsave(1)       = 1.d-1
/

 &stoplist
   tmax=2.0d0
/

 &methodlist
   wnames          = 'rho m1 m2 e'
   fileheadout     = '2Dhd'
   typeadvance     = 'threestep'
   typefull1       = 13*'hllc'
   typelimiter1    = 13*'cada3'
        useprimitive= .true.
        dimsplit    = .false.
        smallp=-1.0d0
        smallrho=-1.0d0

/

 &boundlist
  dixB  =   3
  typeB=
                  4*'periodic',
                  4*'periodic',
                  4*'periodic',
                  4*'periodic'
/
 &amrlist
   mxnest          = 1
   nxlone1         = 32
   nxlone2         = 32
   nxblock1        = 16
   nxblock2        = 16
   xprobmin1       = 0.0d0
   xprobmax1       = 1.0d0
   xprobmin2       = 0.0d0
   xprobmax2       = 1.0d0
   iprob=1
 /

 &paramlist
  slowsteps        = 10
  courantpar       = 1.2d0
  typecourant      = 'maxsum'
 /
