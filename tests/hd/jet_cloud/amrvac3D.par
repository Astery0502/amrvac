!=====================================================
! Project : 3D Jet cloud
! Aim     : de Gouveia dal Pino, ApJ 526, 826 (1999)
! Results :
! Update  : 31/08/2015
! configuration :
! $AMRVAC_DIR/setup.pl -d=33 -phi=0 -z=0 -g=16,16,16 -p=hd -eos=default -nf=1 -ndust=0 -u=nul -arch=default
!=====================================================

 &filelist
        filenamelog='jetCloud3D'
        filenameout='jetCloud3D'
        primnames='rho v1 v2 v3 p tr'
        saveprim=.true.
        autoconvert=.true.
        convert_type='vtuBmpi'
        typeparIO=-1
 /

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
	dtsave(1)=0.05
        dtsave(2)=0.5d0
 /

 &stoplist
        dtmin=1.D-9
        itmax=100
 /
        tmax=6.0d0

 &methodlist
        wnames= 'rho m1 m2 m3 e tr'
        fileheadout=  'jetCloud_hd33'
        typeadvance=  'ssprk54'
        typefull1=    13*'hllc'
        typepred1=    13*'hllc'
        typelimiter1= 13*'mp5'
        typelimited='predictor'
        useprimitive=.true.
        dimsplit=.false.
        fixsmall=.true.
        strictsmall=.false.
        strictgetaux=.false.
        nflatgetaux=2
        smallp=1.0d-14
        smallrho=1.0d-14
 /

 &boundlist
        dixB=    3
        typeB=   6*'special',6*'cont',
                 6*'noinflow',6*'noinflow',
		 6*'noinflow',6*'noinflow'
 /

 &amrlist
        errorestimate=3
        mxnest=1
	flags(7)=2
        flags(1)=1
	flags(2)=2
	wflags(1)=0.8
	wflags(2)=0.2
        nbufferx1=0
        nbufferx2=0
        nbufferx3=0
        tol=13*0.3d0
        tolratio=13*0.3d0
        nxlone1=160
        nxlone2=60
	nxlone3=60
        xprobmin1=-16.0d0
        xprobmax1=16.0d0
        xprobmin2=-6.0d0
        xprobmax2=6.0d0
	xprobmin3=-6.0d0
        xprobmax3=6.0d0
        iprob=1
 /

 &paramlist
        slowsteps=100
        typecourant='maxsum'
        courantpar=0.8d0
 /
