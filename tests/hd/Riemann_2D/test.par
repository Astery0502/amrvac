!setup.pl -d=22 -g=16,16 -p=hd

 &filelist
        primnames= 'rho v1 v2 p'
        base_filename='rm_2d'
        typefilelog='regression_test'
        saveprim=.true.
        autoconvert=.false.
	convert_type='vtuBCCmpi'
/

 &savelist
        dtsave(1)=0.01
 /

 &stoplist
        dtmin=1.0d-6
        time_max=0.5d0
 /

 &methodlist
        w_names=       'rho m1 m2 e'
        fileheadout=  'rm_2d'
 /

 &boundlist
        nghostcells=2
        typeB=4*'cont',4*'cont',4*'cont',4*'cont'
 /

 &meshlist
        refine_criterion=3
        refine_max_level=3
        block_nx1=12
        block_nx2=12
        domain_nx1=24
        domain_nx2=24
        xprobmin1=0.0d0
        xprobmax1=1.0d0
        xprobmin2=0.0d0
        xprobmax2=1.0d0
 /

 &hd_list
        hd_n_tracer = 0
        hd_gamma = 1.4
 /
