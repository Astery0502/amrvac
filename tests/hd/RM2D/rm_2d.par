!setup.pl -d=22 -g=16,16 -p=hd

 &filelist
        primnames= 'rho v1 v2 p'
        typefilelog='regression_test'
        filenamelog='rm_2d'
        saveprim=.true.
        autoconvert=.true.
	convert_type='vtuBCCmpi'
/

 &savelist
        dtsave(1)=0.01
 /

 &stoplist
        dtmin=1.0d-6
        tmax=0.5d0
 /

 &methodlist
        wnames=       'rho m1 m2 e'
        fileheadout=  'rm_2d'
 /

 &boundlist
        dixB=2
        typeB=4*'cont',4*'cont',4*'cont',4*'cont'
 /

 &amrlist
        errorestimate=3
        mxnest=3
        nxlone1=24
        nxlone2=24
        xprobmin1=0.0d0
        xprobmax1=1.0d0
        xprobmin2=0.0d0
        xprobmax2=1.0d0
 /
