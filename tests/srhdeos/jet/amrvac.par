 &filelist
        primnames= 'rho v1 v2 p tr1 epsinf ne ne0 lfac w'
        saveprim=.true.
        autoconvert=.true.
/

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        ditsave(1)=1
        dtsave(2)=0.1
 /

 &stoplist
        tmax=1.
        dtmin=1.0d-9
 /

 &methodlist
        wnames=       'rho s1 s2 e dtr1 Depsinf Dne Dne0 lfac w'
        typeadvance=  'threestep'
        typefull1='hllc'
        typelimiter1='cada3'
        useprimitiveRel=F
        fixsmall=T

 /

 &boundlist
        dixB=2
        typeB=10*'cont'
              10*'cont'
              10*'special'
              10*'cont'
 /

 &amrlist
        errorestimate=0
        mxnest = 1
        nxlone1=400
        nxlone2=600
        xprobmin1=-0.50d0
        xprobmax1= 0.50d0
        xprobmin2=0.0d0
        xprobmax2=1.50d0
 /

 &paramlist
        courantpar=0.6
        slowsteps=100
 /
